{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^nut-(.*)?-(.*)": "nutui-uniapp/components/$1$2/$1$2.vue",
      "^nut-(.*)": "nutui-uniapp/components/$1/$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "",
        "app-plus": {
          "titleNView": false
        },
        "h5": {
          "titleNView": false
        },
        "tabBar": {
          "visible": false
        }
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/my/setting",
      "style": {
        "navigationBarTitleText": "设置"
      }
    },
    {
      "path": "pages/my/about",
      "style": {
        "navigationBarTitleText": "关于"
      }
    }
  ],
  "tabBar": {
    "condition": {
      "current": 0,
      "list": [
        {
          "pagePath": "pages/my/index",
          "visible": true
        }
      ]
    },
    "color": "#bfbfbf",
    "selectedColor": "#1296db",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/images/map_i_no.png",
        "selectedIconPath": "static/images/map_i_yes.png",
        "text": "地图"
      },
      {
        "pagePath": "pages/my/index",
        "iconPath": "static/images/my_i_no.png",
        "selectedIconPath": "static/images/my_i_yes.png",
        "text": "我的"
      }
    ]
  },
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#000000"
  },
  "subPackages": []
}
