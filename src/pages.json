{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^nut-(.*)?-(.*)": "nutui-uniapp/components/$1$2/$1$2.vue",
      "^nut-(.*)": "nutui-uniapp/components/$1/$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "",
        "app-plus": {
          "titleNView": false
        },
        "h5": {
          "titleNView": false
        },
        "tabBar": {
          "visible": false
        }
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/my/setting",
      "style": {
        "navigationBarTitleText": "设置",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/about",
      "style": {
        "navigationBarTitleText": "关于",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/order",
      "style": {
        "navigationBarTitleText": "我的订单",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/address",
      "style": {
        "navigationBarTitleText": "收货地址",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mall/index",
      "style": {
        "navigationBarTitleText": "商城"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mall/detail",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "tabBar": {
    "condition": {
      "current": 0,
      "list": [
        {
          "pagePath": "pages/my/index",
          "visible": true
        }
      ]
    },
    "color": "#bfbfbf",
    "selectedColor": "#1296db",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/images/map_i_no.svg",
        "selectedIconPath": "static/images/map_i_yes.svg",
        "text": "电子罗盘"
      },
      {
        "pagePath": "pages/mall/index",
        "iconPath": "static/images/mall_i_no.svg",
        "selectedIconPath": "static/images/mall_i_yes.svg",
        "text": "好运商城"
      },
      {
        "pagePath": "pages/my/index",
        "iconPath": "static/images/my_i_no.svg",
        "selectedIconPath": "static/images/my_i_yes.svg",
        "text": "个人中心"
      }
    ]
  },
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#000000"
  },
  "subPackages": []
}
